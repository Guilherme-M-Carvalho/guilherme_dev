      "ts-node": "^10.7.0",
      "ts-node-dev": "^2.0.0",
      "typescript": "^5.1.6"
   },
   "dependencies": {
      "@types/cookie-parser": "^1.4.3",
      "@types/crypto-js": "^4.1.1",
      "@types/express-session": "^1.17.7",
      "@types/multer": "^1.4.7",
      "bcrypt": "^5.0.1",
      "cookie-parser": "^1.4.6",
      "cors": "^2.8.5",
      "crypto-js": "^4.1.1",
      "dotenv": "^16.0.3",
      "express": "^4.18.2",
      "express-async-errors": "^3.1.1",
      "express-session": "^1.17.3",
      "generate-password": "^1.7.0",
      "jsonwebtoken": "^9.0.0",
      "mariadb": "^3.1.1",
      "module-alias": "^2.2.2",
